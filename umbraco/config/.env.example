# Umbraco Heartcore Configuration
# Copy this file to .env in the project root and update with your credentials

# Your Umbraco Heartcore project alias
# Found in your Heartcore project settings
UMBRACO_PROJECT_ALIAS=your-project-alias

# Content Delivery API Key
# Generate this in your Heartcore project: Settings â†’ API Keys
UMBRACO_API_KEY=your-api-key-here

# Content Delivery API URL (usually this default value)
UMBRACO_API_URL=https://cdn.umbraco.io

# Optional: Enable/disable Umbraco integration
# Set to 'false' to use default static content
UMBRACO_ENABLED=true

# Optional: Content cache duration in seconds (default: 300 = 5 minutes)
UMBRACO_CACHE_TTL=300

# Optional: Fallback to static content if API fails (default: true)
UMBRACO_FALLBACK_ENABLED=true

# Optional: Log level for Umbraco API client (debug, info, warn, error)
UMBRACO_LOG_LEVEL=info
